{
  "name": "panoptic-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "graphql-to-doc": "docusaurus graphql-to-doc && yarn clean-graphql-to-doc",
    "clean-graphql-to-doc": "find ./docs/subgraph-generated/* -type d ! -name generated.md ! -path ./docs/subgraph-generated/types ! -name objects ! -name interfaces ! -name scalars -exec rimraf {} \\; 2>/dev/null || true;",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus-terminology/parser": "^1.3.0",
    "@docusaurus-terminology/term": "^1.0.0",
    "@docusaurus/core": "^2.2.0",
    "@docusaurus/plugin-google-gtag": "^2.4.3",
    "@docusaurus/preset-classic": "^2.2.0",
    "@graphql-markdown/docusaurus": "^1.24.1",
    "@graphql-tools/url-loader": "^8.0.2",
    "@headlessui/react": "^1.7.17",
    "@mdx-js/react": "^1.6.22",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "clsx": "^1.2.1",
    "graphql": "^16.8.1",
    "hast-util-is-element": "1.1.0",
    "inter-ui": "^3.19.3",
    "postcss": "^8.4.31",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-confetti-explosion": "^2.1.2",
    "react-dom": "^18.2.0",
    "react-hubspot-form": "^1.3.7",
    "react-markdown": "^9.0.0",
    "react-sanfona": "^1.5.0",
    "react-slick": "^0.29.0",
    "react-use-draggable-scroll": "^0.4.7",
    "recharts": "^2.7.2",
    "rehype-katex": "^4.0.0",
    "remark-math": "^3.0.1",
    "slick-carousel": "^1.8.1",
